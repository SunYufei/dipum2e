   "source": [
    "f = imread('Fig0305(a).tif');\n",
    "[M, N] = size(f);\n",
    "[f, revertclass] = tofloat(f);\n",
    "F = fft2(f);\n",
    "sig = 10;\n",
    "H = lpfilter('gaussian', M, N, sig);\n",
    "G = H .* F;\n",
    "g = ifft2(G);\n",
    "g = revertclass(g);\n",
    "subplot(121), imshow(f);\n",
    "subplot(122), imshow(g);"
   ]
